(jbuild_version 1)

(rule (
  (targets (ResetPervasives.ml))
  (deps (ResetPervasives.cppo.ml))
  (action (run ${bin:cppo} -V OCAML:${ocaml_version} ${<} -o ${@}))
))

(rule (
  (targets (ResetStandardLibrary.ml))
  (deps (ResetStandardLibrary.cppo.ml))
  (action (run ${bin:cppo} -V OCAML:${ocaml_version} ${<} -o ${@}))
))

(library (
  (name Reset)
  (flags (:standard -nopervasives -w @A))
))
